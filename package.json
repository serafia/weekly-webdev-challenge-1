{
  "name": "",
  "version": "",
  "scripts": {
    "test": "npm run test:html",
    "test:html": "glob-run nu-html-checker *.html",
    "build": "npm run build:clean && npm run build:sass && npm run build:prefixer && npm run test",
    "build:clean": "rimraf \"css/*\"",
    "build:sass": "sass --sourcemap=none --update sass:css",
    "build:prefixer": "autoprefixer-cli style/style.css",
    "watch": "parallelshell \"npm run watch:sass\" \"npm run watch:autoprefixer\" \"npm run watch:browsersync\" ",
    "watch:sass": "sass --watch sass:css",
    "watch:autoprefixer": "onchange -d 100 libs/style.css -- autoprefixer-cli -o libs/style.prefixed.css libs/style.css",
    "watch:browsersync": "node_modules/.bin/browser-sync start --server --files \"libs/*.css\" \"*.html\""
  },
  "dependencies": {
    "autoprefixer-cli": "^1.0.0",
    "axios": "^0.19.0",
    "braces": "^3.0.2",
    "browser-sync": "^2.26.7",
    "cryptiles": "^4.1.2",
    "debug": "^4.0.1",
    "extend": "^3.0.2",
    "fresh": "^0.5.2",
    "glob-run": "^0.1.7",
    "hoek": "^5.0.3",
    "html-validator-cli": "^3.2.0",
    "lodash": "^4.17.14",
    "mime": "^2.3.1",
    "minimax": "^0.1.1",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2",
    "parsejson": "0.0.3",
    "rimraf": "^2.6.2",
    "sshpk": "^1.14.2",
    "stringstream": "^1.0.0",
    "sudo": "^1.0.3",
    "superagent": "^4.0.0-beta.5",
    "vnu-jar": "^17.12.28"
  }
}
